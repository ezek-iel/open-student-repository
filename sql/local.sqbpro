</sql><sql name="Virtual Tables and Triggers*">CREATE VIRTUAL TABLE resources_fts USING fts5(name, source, details, content=&quot;resource&quot;, tokenize=&quot;porter&quot;);

--CREATE TRIGGER resources_fts_ai AFTER INSERT ON resource BEGIN INSERT INTO resources_fts(name, source, details) VALUES (new.name, new.source, new.details); END;</sql><sql name="Basic FTS Search*">SELECT resource.*, bm25(resources_fts) AS rank
FROM resource 
JOIN resources_fts ON resource.name = resources_fts.name 
WHERE resources_fts MATCH &quot;Data&quot;
ORDER BY rank;
</sql><sql name="SQL 5*"></sql><current_tab id="0"/></tab_sql></sqlb_project>
